{{#each Blogs}}

<div class="row">
    <div class="col s12">
      <div class="card">
        <div class="card-content black-text">
          <span class="card-title">{{Blogs.title}} Written By: {{Users.username}}</span>
          <p>{{Blogs.content}}</p>
        </div>
        <div class="card-action">
          <a href="#">See full post</a>
        </div>
      </div>
    </div>
  </div>
    {{/each}}

    